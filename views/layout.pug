doctype html
html
  head
    title Dashboard - Vivienda Acceso Electrónico
    link(rel='stylesheet' href='/bower_components/bootstrap/dist/css/bootstrap.css')
    link(rel='stylesheet' href='/css/style.css')
  body
    nav.navbar.navbar-inverse
      .container
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          span.navbar-brand Vivienda acceso electrónico
        #navbar.collapse.navbar-collapse
          if user
            ul.nav.navbar-nav
              li
                a(href='/') Lista Usuarios
              li
                a(href='/users/add') Registrar Usuario
          ul.nav.navbar-nav.navbar-right
            if user
              li
                a(href='/admin/logout') Desconectarse
            else
              li
                a(href='/admin/login') Conectarse
    .container
      != messages('message', locals)
      if errors
        each error, i in errors
          div(class='alert alert-danger') #{error.msg}
      block content
      br
      hr
      footer
        p
          b Integrantes:
        ul
          li MEDINA LLANTIRHUAY, Carlos Jhulyan
          li MENDOZA HUAMAN, Waldir
          li PARCO LLANTOY, Jhon Cristhian
    script(src='/bower_components/jquery/dist/jquery.js')
    script(src='/bower_components/bootstrap/dist/js/bootstrap.js')
    script(src='/js/main.js')